{
    "exclude": ["**/*.js"],
    "compilerOptions": {
        "target": "es2016",
        "lib": ["ES2019", "DOM"],
        "jsx": "react-jsx",
        "jsxFactory": "",
        "jsxFragmentFactory": "",
        "jsxImportSource": "",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    }
}
